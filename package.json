{
  "name": "huviz",
  "version": "0.37.0",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "http://github.com/smurp/huviz.git"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.0.0"
  },
  "author": "Shawn Murphy <smurp@smurp.com> (http://smurp.com)",
  "contributors": [
    "Wolf Maul",
    "John Simpson <john.simpson@ualberta.ca>"
  ],
  "config": {
    "port": "5000"
  },
  "main": "lib/huviz.js",
  "dependencies": {
    "@fnando/sparkline": "^0.3.10",
    "async": "~0.2.9",
    "coffeescript": "~1.12.7",
    "components-jqueryui": "components/jqueryui",
    "d3": "~5.15.0",
    "decaffeinate": "^6.1.3",
    "ejs": "2.6.1",
    "express": "~4.17.1",
    "jquery": ">=3.4.0",
    "jquery-simulate-ext": "1.3.0",
    "jsoutline": "git://github.com/smurp/jsoutline.git",
    "ldf-client": "^2.0.5",
    "marked": "^0.3.5",
    "morgan": "^1.9.1",
    "nopt": "~3.0.0",
    "quaff-lod": "smurp/quaff-lod",
    "stitch": "~0.3.3"
  },
  "commentedOutDependencies": {},
  "devDependencies": {
    "browserify": "^16.2.3",
    "chai": "~1.10.0",
    "coffeelint": "~1.7.1",
    "docco": "^0.8.0",
    "eslint": "^5.16.0",
    "grunt": "~0.4.5",
    "grunt-cli": "1.2.0",
    "grunt-contrib-coffee": "~0.12.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-express-server": "~0.4.19",
    "grunt-mocha-test": "~0.12.4",
    "grunt-release": "0.9.0",
    "grunt-stitch": "0.1.0",
    "mocha": "~2.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.3.0",
    "onchange": "^7.1.0"
  },
  "watch": {
    "test": {
      "patterns": [
        "js",
        "test",
        "lib",
        "vendor"
      ],
      "extensions": [
        "js",
        "coffee"
      ],
      "quiet": true
    }
  },
  "config": {
    "jsInOrder": "src/angliciser.js src/greenerturtle.js src/indexeddbservice.js src/indexeddbstoragecontroller.js src/graphcommandlanguage.js src/querymanager.js src/uniquer.js src/treepicker.js src/fsm.js src/editui.js src/coloredtreepicker.js src/treectrl.js src/predicate.js src/taxon.js src/sortedset.js src/gclui.js src/hsv.js src/hsl.js src/edge.js src/node.js src/textcursor.js src/quadParser.js src/multistring.js src/oncerunner.js src/huviz.js"
  },
  "scripts": {
    "OLD:watch:coffee": "nodemon -w src/huviz.coffee --exec 'decaffeinate --loose-for-of  < src/huviz.coffee > src/huviz.js'",
    "OLD:watch:js": "nodemon -q -w src/*.js lib/huviz_decaf.js --exec 'npm run build:js'",
    "OLD:build:coffee": "decaffeinate --loose < src/huviz.coffee > src/huviz.js",
    "OLD:watch:coffee": "onchange 'src/huviz.coffee' 'package.json' -- npm run build:coffee",
    "build:js": "cat $npm_package_config_jsInOrder > lib/huviz.js && echo \"export {Orlando};\" >> lib/huviz.js",
    "watch:js": "onchange --await-write-finish 2000 'src/*.js' -- npm run build:js",
    "watch": "npm-run-all --parallel watch:*",
    "dev": "onchange 'lib/huviz.js' -- npm start",
    "start": "node server.js --git_commit_hash `git log --format=\"%h\" -n 1` --git_branch_name `git rev-parse --abbrev-ref HEAD`",
    "test": "node_modules/mocha/bin/mocha --require coffeescript/register --compilers coffee:coffeescript --reporter dot",
    "buildSrcDocs": "npx docco -o srcdocs  server.coffee src/*.coffee | node bin/make_srcdocs_index.js > srcdocs/index.md && npx marked srcdocs/index.md > srcdocs/index.html",
    "watchTest": "npm-watch"
  }
}
